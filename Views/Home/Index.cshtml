<script src="//code.responsivevoice.org/responsivevoice.js?key=3G053CLH"></script>

<script>
    function speak(input) {
        responsiveVoice.speak(input, "Portuguese Female");
    }

    function wait(ms) {
    var d = new Date();
    var d2 = null;
    do { d2 = new Date(); }
    while (d2 - d < ms);
    }

    function executeAsync(func) {
    setTimeout(func, 0);
    }


</script>


