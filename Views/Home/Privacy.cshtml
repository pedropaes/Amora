@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<script src="https://cdnjs.cloudflare.com/ajax/libs/annyang/2.6.0/annyang.min.js"></script>

<script>

    window.onload = function () {

        //annyang.setLanguage('pt-PT');
        annyang.start({ autoRestart: true, continuous: false });
        if (annyang) {
            // Let's define a command.
            
            console.log("It detected annyang");
            
            var commands = {
                'find *tag':procura,
                'caralho': function () { alert('caralho!!!!' ); }
            };

            var procura = function (tag) {
                alert('ai o caralho:' + tag);
                //document.getElementById('procura').setAttribute('value', tag);
                //document.getElementById('procurar').click();
            };
            // Add our commands to annyang
            annyang.addCommands(commands);

            // Start listening.
            annyang.start();
        }
        
    }
    
</script>